#! /usr/bin/env bash

if [ -z $PROJECT ]; then
    echo "Active virtual session not found. See docs."
    exit
fi

rm -rf $PROJECT/bin/easy* $PROJECT/bin/activate* $PROJECT/bin/pip*
rm -rf $PROJECT/bin/wheel* $PROJECT/bin/python* $PROJECT/bin/flask*
rm -rf $PROJECT/pyvenv.cfg $PROJECT/build/ $PROJECT/dist/ $PROJECT/lib/
rm -rf $PROJECT/__pycache__/ $PROJECT/src/__pycache__/ $PROJECT/bin/camera
rm -rf $PROJECT/camera.spec $PROJECT/bin/pyserial* $PROJECT/bin/pyinstall*
